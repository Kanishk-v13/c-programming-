#include <stdio.h>

int main() {
    int length, breadth, area, perimeter;

    printf("Enter length: ");
    scanf("%d", &length);
    printf("Enter breadth: ");
    scanf("%d", &breadth);

    area = length * breadth;
    perimeter = 2 * (length + breadth);

    printf("Area = %d, Perimeter = %d\n", area, perimeter);

    return 0;
}
